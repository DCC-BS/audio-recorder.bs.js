{
    "name": "@dcc-bs/audio-recorder.bs.js",
    "description": "Audio Recorder is a powerful Nuxt.js module that provides advanced audio recording, processing, and management capabilities for web applications.",
    "version": "2.0.1",
    "license": "MIT",
    "type": "module",
    "homepage": "https://dcc-bs.github.io/documentation/",
    "author": {
        "name": "Data Competence Center Basel Stadt",
        "email": "dcc@bs.ch",
        "url": "https://www.bs.ch/daten/databs/dcc"
    },
    "keywords": [
        "nuxt",
        "audio",
        "recorder",
        "ffmpeg",
        "nuxt-module",
        "mp3"
    ],
    "contributors": [
        {
            "email": "yanick.schraner@bs.ch",
            "name": "Yanick Schraner"
        },
        {
            "email": "tobias.bollinger@bs.ch",
            "name": "Tobias Bollinger"
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/DCC-BS/audio-recorder.bs.js"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/"
    },
    "exports": {
        ".": {
            "types": "./dist/types.d.mts",
            "import": "./dist/module.mjs",
            "require": "./dist/module.mjs",
            "style": "./dist/main.css"
        },
        "./prelude": {
            "types": "./dist/runtime/prelude.d.ts",
            "import": "./dist/runtime/prelude.js",
            "require": "./dist/runtime/prelude.js"
        }
    },
    "main": "./dist/module.mjs",
    "types": "./dist/types.d.mts",
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "prepack": "bunx nuxt-module-build build",
        "generate": "nuxi generate",
        "dev": "nuxi dev playground",
        "dev:build": "nuxi build playground",
        "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
        "release": "bun lint && bun test && bun prepack && changelogen --release && git push --follow-tags",
        "lint": "biome format --write",
        "check": "biome check --fix",
        "test": "vitest",
        "test:watch": "vitest --watch",
        "test:coverage": "vitest run --coverage",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui"
    },
    "dependencies": {
        "@ffmpeg/ffmpeg": "0.12.15",
        "@ffmpeg/util": "0.12.2",
        "@nuxt/kit": "4.2.2",
        "@nuxtjs/i18n": "10.2.3",
        "dexie": "4.2.1",
        "rollup-plugin-import-css": "4.2.0"
    },
    "devDependencies": {
        "@biomejs/biome": "2.3.14",
        "@nuxt/devtools": "3.1.1",
        "@nuxt/module-builder": "1.0.2",
        "@nuxt/schema": "4.2.2",
        "@nuxt/test-utils": "3.21.0",
        "@playwright/test": "1.58.2",
        "@types/node": "25.0.10",
        "@vitest/coverage-v8": "4.0.17",
        "changelogen": "0.6.2",
        "happy-dom": "20.6.1",
        "nuxt": "4.2.2",
        "publish-to-git": "1.2.0",
        "typescript": "5.9.3",
        "unbuild": "3.6.1",
        "vitest": "4.0.17",
        "vitest-environment-nuxt": "1.0.1",
        "vue-tsc": "3.2.2"
    },
    "peerDependencies": {
        "@nuxt/ui": "4.4.0",
        "motion-v": "1.7.4"
    }
}
